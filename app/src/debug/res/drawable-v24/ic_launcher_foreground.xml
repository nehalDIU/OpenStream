        <path android:pathData="M-18,-8 C-18,-12 -15,-15 -11,-15 C-9,-17 -6,-18 -3,-18 C2,-18 6,-14 6,-9 C9,-9 12,-6 12,-3 C12,1 9,4 5,4 L-15,4 C-17,4 -18,2 -18,-1 Z"
              android:fillColor="#ffffff"
              android:fillAlpha="0.95"/>

        <!-- Cloud highlight -->
        <path android:pathData="M-15,-6 C-15,-9 -13,-11 -10,-11 C-8,-12 -6,-13 -4,-13 C0,-13 3,-10 3,-6 C5,-6 7,-4 7,-2 C7,0 5,2 3,2 L-12,2 C-14,2 -15,1 -15,-1 Z"
              android:fillColor="#ffffff"
              android:fillAlpha="0.3"/>

        <!-- Streaming lines -->
        <path android:pathData="M-8,8 L8,8"
              android:strokeColor="#ffffff"
              android:strokeWidth="2"
              android:strokeLineCap="round"
              android:strokeAlpha="0.8"/>
        <path android:pathData="M-6,12 L6,12"
              android:strokeColor="#ffffff"
              android:strokeWidth="2"
              android:strokeLineCap="round"
              android:strokeAlpha="0.6"/>
        <path android:pathData="M-4,16 L4,16"
              android:strokeColor="#ffffff"
              android:strokeWidth="2"
              android:strokeLineCap="round"
              android:strokeAlpha="0.4"/>

        <!-- Debug indicator "D" -->
        <path android:pathData="M14,-12 L14,-4 M14,-12 C16,-12 18,-10 18,-8 C18,-6 16,-4 14,-4"
              android:strokeColor="#ffffff"
              android:strokeWidth="1.5"
              android:strokeLineCap="round"
              android:strokeAlpha="0.9"/>
    </group>

    <!-- Subtle shadow effect -->
    <path android:pathData="M54,56m-43,0a43,43 0,1 1,86 0a43,43 0,1 1,-86 0"
          android:fillColor="#000000"
          android:fillAlpha="0.1"/>
